#include<stdio.h>
int main()
{
 int m,n,p,q,i,j,k,a[10][10],b[10][10],c[10][10];
 printf("Enter the Size of matrix A\n");
 scanf("%d%d",&m,&n);
 printf("Enter the Size of matrix B\n");
 scanf("%d%d",&p,&q);
 if(n!=p)
 {
   printf("MATRIX MULTIPLICATION IS NOT POSSIBLE\n");
 }
else
 {
   printf("ENTER THE ELEMENTS OF MATRIX A\n");
    for(i=0;i<m;i++)
     {
       for(j=0;j<n;j++)
        scanf("%d",&a[i][j]);
     }
   printf("ENTER THE ELEMENTS OF MATRIX B\n");
    for(i=0;i<p;i++)
     {
       for(j=0;j<p;j++)
        scanf("%d",&b[i][j]);
     }
   for(i=0;i<m;i++)
    {
      for(i=0;j<q;j++)
       {
         c[i][j]=0;
         for(k=0;k<n;k++)
         c[i][j]=c[i][j]+a[i][k]*b[k][j];
      }
    }
  printf("A-matrix is\n");
   for(i=0;i<m;i++)
    {
      for(j=0;j<n;j++)
       {
         printf("%d\t",a[i][j]);
       }
      printf("\n");
    }
 printf("THE RESULTANT MATRIX OF C IS\n");
  for(i=0;i<m;i++)
   {
     for(j=0;j<q;j++)
      {
        printf("%d\t",c[i][j]);
      }
    printf("\n");
   }
 }
}


 
